using System;

namespace ConsoleApp6
{
    internal class Program
    {
        static void Main(string[] args)
        {
           string str= Console.ReadLine();

            string res = " ";
            //for (int i = 0; i < str.Length; i++)
            for (int i = str.Length-1; i >=0 ; i--) 
            {
                res += str[i];
            }
            Console.WriteLine(res);
            Console.ReadLine();
        }

    }

    
}
